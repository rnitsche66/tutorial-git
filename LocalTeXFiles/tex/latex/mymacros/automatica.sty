cross-references right}\fi\fi\endgroup\deadcycles\z@\@@end}

\def\@testbibh #1#2#3{
  \def\@tempa{#2}\expandafter
  \ifx \csname bhf@#1\endcsname \@tempa
     \def\@tempa{#3}\expandafter
     \ifx \csname bha@#1\endcsname \@tempa
     \else \@tempswatrue
     \fi
  \else
     \@tempswatrue
  \fi
}

